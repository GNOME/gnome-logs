msgid "%b %e %l:%M %p"
msgstr "%e %b %l:%M %p"

#. Translators: timestamp format for events in a different
#. * year, showing the abbreviated month name, day of the
#. * month, year and the time in 12-hour format.
#: ../src/gl-util.c:107
msgid "%b %e %Y %l:%M %p"
msgstr "%e %b %Y %l:%M %p"

