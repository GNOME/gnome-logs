    <licence>CC0</licence>
<!-- Both taken from desktop file.
    <name>Logs</name>
    <summary>View detailed event logs for the system</summary>
-->
    <description>
        <_p>Logs shows events from the systemd journal, and sorts them into
        categories, such as hardware and applications.</_p>
        <_p>Using Logs you can search your logs by typing a search term, and
        view detailed information about each event by clicking on it.</_p>
    </description>
    <screenshots>
        <_screenshot type="default" width="1000" height="575">http://amigadave.com/temp/gnome-logs-default.png</_screenshot>
    </screenshots>
    <url type="homepage">https://wiki.gnome.org/Apps/Logs</url>
    <updatecontact>davidk@gnome.org</updatecontact>
    <project_group>GNOME</project_group>
</application>
